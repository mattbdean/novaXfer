// NVCC class that has equivalencies at VT and UVA
{
    "subject": "ECO",
    "number": "202",
    "equivalencies": [
        {
            "institution": "VT",
            "type": "direct",
            "input": [
                {
                    "subject": "ECO",
                    "number": "202",
                    "credits": 3
                },
            ],
            "output": [
                {
                    "subject": "ECON",
                    "number": "2005",
                    "credits": 3
                }
            ]
        },
        {
            "institution": "UVA",
            "input": [
                {
                    "subject": "ECO",
                    "number": "202",
                    "credits": 3
                }
            ],
            "output": [
                {
                    "number": "2010",
                    "subject": "ECON",
                    "credits": 3,
                }
            ]
        }
    ]
}

// NVCC class that when taken with another class grants additional credit at UVA
// ex: ACC 211 taken alone grants 3 credits, ACC 211 + ACC 213 grants 4
{
    "subject": "ACC",
    "number": "211",
    "equivalencies": [
        {
            "institution": "UVA",
            "type": "direct",
            "input": [
                {
                    "subject": "ACC",
                    "number": "211",
                    "credits": 3
                }
            ],
            "output": [
                {
                    "subject": "COMM",
                    "number": "2010",
                    "credits": 3
                }
            ]
        },
        {
            "institution": "UVA",
            "input": [
                {
                    "subject": "ACC",
                    "number": "211",
                    "credits": 3
                },
                {
                    "subject": "ACC",
                    "number": "213",
                    "credits": 3
                }
            ],
            "output": [
                {
                    "subject": "COMM",
                    "number": "2010",
                    "credits": 4
                }
            ]
        }
    ]
}

// NVCC class that grants credit for two classes
{
    "subject": "CSC",
    "number": "201",
    "equivalencies": [
        {
            "institution": "GT",
            "type": "generic",
            "input": [
                {
                    "subject": "CSC",
                    "number": "201",
                    "credits": 4,
                }
            ],
            "output": [
                {
                    "subject": "CS",
                    "number": "1301",
                    "credits": 3,
                },
                {
                    "subject": "FREE",
                    "number": "1XXX",
                    "credits": 1
                }
            ]
        }
    ]
}

