// NVCC class that has equivalencies at VT and UVA
{
    "subject": "ECO",
    "number": "202",
    "credits": 3,
    "equivalencies": [
        {
            "institution": "Virginia Tech",
            "subject": "ECON",
            "number": "2005",
            "credits": 3
        },
        {
            "institution": "University of Virginia",
            "number": "2010",
            "subject": "ECON",
            "credits": 3,
        }
    ]
}

// NVCC class that when taken with another class grants additional credit at UVA
// ex: ACC 211 taken alone grants 3 credits, ACC 211 + ACC 213 grants 4
{
    "subject": "ACC",
    "number": "211",
    "credits": 3,
    "equivalencies": [
        {
            "institution": "University of Virginia",
            "subject": "COMM",
            "number": "2010",
            "credits": 3
        },
        {
            "institution": "University of Virginia",
            "subject": "COMM",
            "number": "2010",
            "credits": 4,
            "supplement": {
                "subject": "ACC",
                "number": "213"
            }
        }
    ]
}

